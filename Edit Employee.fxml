<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="599.0" prefWidth="862.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="mainpkg.EditEmployeeController">
    <stylesheets>
        <URL value="@edit employee.css" />
    </stylesheets>
   <children>
      <TableView fx:id="EditEmployeeTableView" layoutY="160.0" prefHeight="343.0" prefWidth="839.0">
        <columns>
          <TableColumn fx:id="NameEmployee" prefWidth="110.0" text="Name" />
          <TableColumn fx:id="IDEmployee" minWidth="9.0" prefWidth="144.0" text="ID" />
            <TableColumn fx:id="EmailEmployee" minWidth="9.0" prefWidth="144.0" text="E-mail" />
            <TableColumn fx:id="GenderEmployee" minWidth="9.0" prefWidth="144.0" text="Gender" />
            <TableColumn fx:id="PhoneNumberEmployee" minWidth="9.0" prefWidth="144.0" text="Phone Number" />
            <TableColumn fx:id="DepartmentEmployee" minWidth="9.0" prefWidth="144.0" text="Department" />
        </columns>
      </TableView>
      <Button fx:id="AddEmployeeButton" layoutX="14.0" layoutY="65.0" mnemonicParsing="false" onAction="#AddEmployeeButtonOnClicked" text="Add Employee">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="RemoveEmployeeButton" layoutX="14.0" layoutY="111.0" mnemonicParsing="false" onAction="#RemoveEmployeeButtonOnClicked" text="Remove Employee">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="BackButton" layoutX="23.0" layoutY="541.0" mnemonicParsing="false" onAction="#BackButtonOnClicked" prefHeight="25.0" prefWidth="66.0" text="Back">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button fx:id="LoadButton" layoutX="586.0" layoutY="539.0" mnemonicParsing="false" onAction="#LoadbuttonOnClicked" prefHeight="29.0" prefWidth="102.0" text="Load ">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Text layoutX="234.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Edit Employee" underline="true">
         <font>
            <Font name="Bauhaus 93" size="24.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
